{
  "source": {
    "include": ["src", "package.json", "README.md"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark",
      "title": "GhostSelfBot Documentation",
      "menu": [
        {
          "title": "GitHub",
          "link": "https://github.com/ghostnet-team/ghostselfbot",
          "target": "_blank"
        },
        {
          "title": "NPM",
          "link": "https://www.npmjs.com/package/ghostselfbot",
          "target": "_blank"
        }
      ],
      "meta": [
        {
          "name": "author",
          "content": "GhostNet Team"
        },
        {
          "name": "description",
          "content": "A powerful Discord self-bot library with advanced features and optimized performance"
        }
      ],
      "search": true,
      "collapse": true,
      "stylesheets": [
        "./custom.css"
      ],
      "footer": "GhostNet Team Â© 2025-2026"
    }
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-mermaid"
  ],
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "includeDate": false,
      "outputSourceFiles": true,
      "staticFiles": {
        "include": [
          "./custom.css"
        ]
      }
    }
  },
  "metadata": {
    "title": "GhostSelfBot Documentation"
  },
  "docdash": {
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "GitHub": {
        "href": "https://github.com/ghostnet-team/ghostselfbot",
        "target": "_blank"
      }
    }
  }
} 